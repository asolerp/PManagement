# ========================================
# Android Signing Configuration
# ========================================

# Path al archivo keystore (relativo a android/app/)
ANDROID_KEYSTORE_PATH=portmanagement.keystore

# Password del keystore
ANDROID_KEYSTORE_PASSWORD=your_keystore_password_here

# Alias de la key dentro del keystore
ANDROID_KEY_ALIAS=portmanagement

# Password de la key
ANDROID_KEY_PASSWORD=your_key_password_here

